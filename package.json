{
  "name": "@lorhansohaky/react-metismenu",
  "version": "1.4.0",
  "description": "A ready / simple to use, highly customizable, updateable, ajax supported, animated and material designed menu component for React",
  "main": "dist/react-metismenu.js",
  "private": false,
  "scripts": {
    "start": "webpack-dev-server --config webpack/dev.conf.js",
    "build": "webpack",
    "unit-test": "jest",
    "coverage": "jest --coverage",
    "coveralls": "coveralls < coverage/lcov.info",
    "test": "npm run lint-src && npm run coverage"
  },
  "jest": {
    "testRegex": "/__tests__/.*\\.jsx?$",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alpertuna/react-metismenu.git"
  },
  "keywords": [
    "react",
    "metis",
    "menu",
    "component",
    "json",
    "collapsable",
    "animated",
    "smooth",
    "customizable",
    "ajax",
    "material"
  ],
  "author": "H.Alper Tuna <halpertuna@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/LorhanSohaky/react-metismenu/issues"
  },
  "homepage": "https://github.com/LorhanSohaky/react-metismenu#readme",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "babel-loader": "^9.1.3",
    "clean-css-cli": "^5.6.3",
    "coveralls": "^3.1.1",
    "css-loader": "^6.10.0",
    "jest": "^29.7.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "less-plugin-clean-css": "^1.5.1",
    "node-sass": "^9.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-test-renderer": "^17.0.0",
    "style-loader": "^3.3.4",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2"
  },
  "peerDependencies": {
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "dependencies": {
    "classnames": "^2.5.1",
    "prop-types": "^15.8.1",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "simple-ajax": "^2.6.0"
  }
}
